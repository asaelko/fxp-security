{
    "name": "sonatra/security",
    "type": "library",
    "description": "Implements all features of Symfony2 Advanced ACL Concepts and adds some interesting features",
    "keywords": ["ACL", "ACE", "Advanced ACL Concept Implementation", "Rule definition", "Sharing rule", "ACL Voter", "JMS Expression"],
    "homepage": "https://github.com/sonatra/sonatra-security",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/security": "~3.0",
        "symfony/security-acl": "~3.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/expression-language": "~3.0",
        "symfony/validator": "~3.0",
        "doctrine/common": "~2.5",
        "psr/cache": "~1.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "~3.0",
        "doctrine/orm": "~2.5"
    },
    "suggest": {
        "doctrine/orm": "For ORM filter query feature"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\Security\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
